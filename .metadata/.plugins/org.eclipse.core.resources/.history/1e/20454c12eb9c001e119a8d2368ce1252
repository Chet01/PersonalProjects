package practise;

public class ArmstrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int num=371;
      int copy=371;
      int lastDigit=0;
      int result=0;
      while(num!=0) {
    	  lastDigit = num%10;
    	  result+= Math.pow(lastDigit, 3);
    	  copy=copy/10;
      }
      if(copy==num) {
    	  System.out.println("It is an Armsttrong number" + num);
      }
      
      
	}

}
