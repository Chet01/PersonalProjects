package parctice;

public class CheckForPalindrome {

	public static void main(String[] args) {
		// check for
		String pal = "madam";
		String res ="";
		for(int i=pal.length()-1;i>=0;i--) {
			res+=pal.charAt(i);
		}
        if(res==pal) {
        	System.out.println("String is palindrome");
        }
	}

}
